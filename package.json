    "babel-eslint": "^8.0.2",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "env-test": "^1.0.0",
    "eslint": "^4.19.1",
    "eslint-config-google": "^0.9.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-mocha": "^4.11.0",
    "eslint-watch": "^3.1.3",
    "mocha": "^5.2.0",
    "npm-run-all": "^4.1.2",
    "should": "^13.1.3",
    "supertest": "^3.0.0"
