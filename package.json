{
  "name": "hacker-bay-backend",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": "10.4.1",
    "npm": "6.1.0"
  },
  "scripts": {
    "prestart": "npm run -s build",
    "start": "node dist/app",
    "dev": "DEBUG=email-templates && nodemon src/app --exec \npm run lint && node -r babel -register\"",
    "clean": "rimraf dist",
    "build": "npm run clean && mkdir dist && babel src -s -d dist",
    "test": "mocha --opts ./mocha.opts __test__/**/*/*.spec.js",
    "test:current": "mocha --opts ./mocha.opts __test__/e2e/**/google.spec.js",
    "lint": "esw src"
  },
  "dependencies": {
    "app-root-path": "^2.1.0",
    "assert": "^1.4.1",
    "babel-eslint": "^10.0.1",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.18.3",
    "cloudinary": "^1.13.2",
    "config": "^3.0.0",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "express-fileupload": "^1.0.0",
    "express-upload": "^0.1.0",
    "faker": "^4.1.0",
    "fs": "0.0.1-security",
    "jsonwebtoken": "^8.4.0",
    "lodash": "^4.17.21",
    "mkdirp": "^0.5.1",
    "mongoose": "^5.3.14",
    "mongoose-hidden": "^1.6.0",
    "morgan": "^1.9.1",
    "multer": "^1.4.1",
    "path": "^0.12.7",
    "q": "^1.5.1",
    "request": "^2.88.0",
    "request-promise": "^4.2.2",
    "rimraf": "^2.6.2",
    "slugify": "^1.3.3",
    "swagger-jsdoc": "^3.2.5",
